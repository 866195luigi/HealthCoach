<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HealthCoach" parent="Theme.AppCompat.DayNight.NoActionBar">-
        <!-- Primary brand color. -->
        <item name="colorOnPrimary">@color/green_light</item>
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/green_dark</item>
        <!-- Secondary brand color. UNUSED -->
        <item name="colorOnSecondary">@color/material_black</item>
        <item name="colorSecondary">@color/material_teal_200</item>
        <item name="colorSecondaryVariant">@color/material_teal_700</item>
        <!-- Text & BackgroundColor -->
        <item name="textFillColor">@color/dark</item>
        <item name="backgroundColor">@color/light</item>
        <item name="backgroundInsetTop">@color/material_white</item>
    </style>
</resources>
